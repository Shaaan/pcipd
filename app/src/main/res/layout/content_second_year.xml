<?xml version="1.0" encoding="utf-8"?>
<android.support.v4.widget.NestedScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:iosched="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    tools:context="in.shaaan.pcipharmd.SecondYear"
    tools:showIn="@layout/activity_second_year">


    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"
        android:padding="10dp"
        android:layout_marginTop="30dp">

        <android.support.v7.widget.CardView
            android:layout_width="match_parent"
            android:layout_height="60dp"
            iosched:cardCornerRadius="@dimen/card_corner_radius"
            iosched:cardElevation="@dimen/card_elevation"
            android:layout_marginTop="5dp"
            android:layout_marginBottom="5dp"
            iosched:cardPreventCornerOverlap="false">
        <Button
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:text="@string/cology"
            android:padding="10dp"
            style="@style/AppTheme.Button"
            android:background="@android:color/transparent"
            android:id="@+id/cology"/>
        </android.support.v7.widget.CardView>

        <android.support.v7.widget.CardView
            android:layout_width="match_parent"
            android:layout_height="60dp"
            iosched:cardCornerRadius="@dimen/card_corner_radius"
            iosched:cardElevation="@dimen/card_elevation"
            android:layout_marginTop="5dp"
            android:layout_marginBottom="5dp"
            iosched:cardPreventCornerOverlap="false">
        <Button
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:text="@string/cognosy"
            android:padding="10dp"
            style="@style/AppTheme.Button"
            android:background="@android:color/transparent"
            android:id="@+id/cognosy"/>
        </android.support.v7.widget.CardView>

        <android.support.v7.widget.CardView
            android:layout_width="match_parent"
            android:layout_height="60dp"
            iosched:cardCornerRadius="@dimen/card_corner_radius"
            iosched:cardElevation="@dimen/card_elevation"
            android:layout_marginTop="5dp"
            android:layout_marginBottom="5dp"
            iosched:cardPreventCornerOverlap="false">
        <Button
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:text="@string/tp1"
            android:padding="10dp"
            style="@style/AppTheme.Button"
            android:background="@android:color/transparent"
            android:id="@+id/tp1"/>
        </android.support.v7.widget.CardView>

        <android.support.v7.widget.CardView
            android:layout_width="match_parent"
            android:layout_height="60dp"
            iosched:cardCornerRadius="@dimen/card_corner_radius"
            iosched:cardElevation="@dimen/card_elevation"
            android:layout_marginTop="5dp"
            android:layout_marginBottom="5dp"
            iosched:cardPreventCornerOverlap="false">
        <Button
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:text="@string/micro"
            android:padding="10dp"
            style="@style/AppTheme.Button"
            android:background="@android:color/transparent"
            android:id="@+id/micro"/>
        </android.support.v7.widget.CardView>

        <android.support.v7.widget.CardView
            android:layout_width="match_parent"
            android:layout_height="60dp"
            iosched:cardCornerRadius="@dimen/card_corner_radius"
            iosched:cardElevation="@dimen/card_elevation"
            android:layout_marginTop="5dp"
            android:layout_marginBottom="5dp"
            iosched:cardPreventCornerOverlap="false">
        <Button
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:text="@string/patho"
            android:padding="10dp"
            style="@style/AppTheme.Button"
            android:background="@android:color/transparent"
            android:id="@+id/patho"/>
        </android.support.v7.widget.CardView>

        <android.support.v7.widget.CardView
            android:layout_width="match_parent"
            android:layout_height="60dp"
            iosched:cardCornerRadius="@dimen/card_corner_radius"
            iosched:cardElevation="@dimen/card_elevation"
            android:layout_marginTop="5dp"
            android:layout_marginBottom="5dp"
            iosched:cardPreventCornerOverlap="false">
        <Button
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:text="@string/com"
            android:padding="10dp"
            style="@style/AppTheme.Button"
            android:background="@android:color/transparent"
            android:id="@+id/com"/>
        </android.support.v7.widget.CardView>
    </LinearLayout>

</android.support.v4.widget.NestedScrollView>
